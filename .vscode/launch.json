    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch (Mono)",
            "type": "mono",
            "request": "launch",
            "runtimeExecutable": "${workspaceRoot}/debug-wrapper.sh",
            "program": "${workspaceRoot}/bin/Debug/net8.0/osx-arm64/efcorerepro",
            "cwd": "${workspaceRoot}",
            "env": {},
        },
        {
            "name": "Attach",
            "type": "mono",
            "request": "attach",
            "address": "localhost",
            "port": 55555
        }
    ]
}